<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiAppAntikhovitch.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiAppAntikhovitch"
    xmlns:view="clr-namespace:MauiAppAntikhovitch.View"
    Shell.FlyoutBehavior="Flyout"
    Title="MauiAppAntikhovitch">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Calculator"
        ContentTemplate="{DataTemplate view:CalculatorPage}"
        Route="CalculatorPage" />
    <ShellContent
        Title="Integral calculation"
        ContentTemplate="{DataTemplate view:IntegralCalculationPage}"
        Route="IntegralCalculationPage" />
</Shell>
